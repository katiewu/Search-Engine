         htmlid: htmlid,                                    adSizeType: AdSizeType.NotAnAd,                                    canDisplayAdChoices: false,                                    elem: elem                                };                                                                checkAndSetAdContainerVisibility(renderData);                            }                            function checkAndSetAdContainerVisibility(renderData)                            {                                var retries = numEvalPasses;                                checkAndSetAdContainerVisibilityRec();                                function checkAndSetAdContainerVisibilityRec(finalCheck)                                {                                    retries--;                                    var adId = renderData.htmlid;                                    checkVisibilityAndUpdateRenderDataContextForElement(renderData);                                    var isLastPass = (retries === 0);                                    var adSizeType = renderData.adSizeType;                                    var adDetected = adSizeType !== AdSizeType.Inconclusive;                                    var doShow = adSizeType === AdSizeType.NonPointSizedAd;                                    if (adDetected || isLastPass)                                    {                                        var adSizeClassname = postEvaluationNoAdClassname;                                        if (doShow)                                        {                                            if (renderData.height < mediumAdHeight)                                            {                                                adSizeClassname = postEvaluationAdSmallClassname;                                            }                                            else if (renderData.height < largeAdHeight)                                            {                                           