                  function addOrRemoveClassname(classNameList, className, add)                            {                                var classIndex = classNameList.indexOf(className);                                if (add)                                {                                    if (classIndex === -1)                                    {                                        return classNameList + " " + className;                                    }                                }                                else if (classIndex >= 0)                                {                                    return classNameList.replace(className, "");                                }                                return classNameList;                            }                            window.dap = dapResult;                        })();                        //]]>                            dap("&amp;AP=1089&amp;PG=FINUSEN44&amp;PVGUID=30977b7c1f7a4888be4b09f67a6e74cf&amp;PROVIDERID=7GYW5Y6", 300, 250, "rectangle6_article_container_b628793b-c1c4-4eef-96ad-c32555b23c8c");                    </script>            </div>                    <a href="http://go.microsoft.com/fwlink/?LinkID=286759" target="_blank" class="adchoices" data-piitxt="adchoices">                        <span>AdChoices</span>                    </a>        </div>    </div>    <div  class="ad"  id="button1_article_2ad89ed2-f2e0-4836-9eaf-bed2dec65997">        <div>            <div  id="button1_article_container_2ad89ed2-f2e0-4836-9eaf-bed2dec65997">                    <script type="text/javascript">                        //<![CDATA[                        (function define_dap()                         {                            if (window.dap)                            {                                return;                            }                            // all data needed to render the ads or refresh them                            var postEvaluationClassname =